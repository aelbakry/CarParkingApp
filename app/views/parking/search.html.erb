<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<link rel="stylesheet" href="parkings.scss">


<%= render "shared/header" %>
<div class="card-columns">
<% @results.each do |result| %>
<div class="card" >
  <span class="label label-primary"> <%= result.empty_spaces %></span>
  <div class="card-body">
    <h5 class="card-title"> <%= result.name %> </h5>
    <p class="card-text"> <%= result.location %> <%= result.postcode %> </p>
    <div class="btn btn-primary" >  <%=  link_to 'View', book_path( result.id ) %> </div>
  </div>
</div>
<% end %>
</div>
